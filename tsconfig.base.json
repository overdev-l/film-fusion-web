{
    "compilerOptions": {
        "target": "ES6",
        "module": "ESNext",
        "moduleResolution": "node",
        "lib": ["ESNext", "DOM"],
        "noEmit": true,
        "strict": true,
        "isolatedModules": true,
        "noUnusedLocals": true,
        "noUnusedParameters": true,
        "baseUrl": ".",
        "paths": {
            "@server": ["packages/server/src"],
        }
    },
}